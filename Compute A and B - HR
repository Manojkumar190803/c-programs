#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() {

    /* Enter your code here. Read input from STDIN. Print output to STDOUT */    
    int b,e;
    long double r= 1.0;
    scanf("%d%d",&b,&e);
    while(e!=0){
        r=r*b;
        --e;
    }
    printf("%.0Lf",r);
    
    return 0;
}
