#include <cmath>
#include <cstdio>
#include <vector>
#include <iostream>
#include <algorithm>
using namespace std;

int main() {
    int n,s=0;cin>>n;
    int t=n;
    while(t!=0){
        s=s+t%10;
        t/=10;
    }
    if(n%s==0)
        cout<<"Yes\n";
    else
        cout<<"No\n";
    return 0;
}
